<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>{% block title %}{% endblock title %}</title>
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<link href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css" rel="stylesheet">
	{% stylesheets 'bundles/moviesadmin/css/*' filter='cssrewrite' %}
	    <link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
	{% block stylesheets %}{% endblock stylesheets %}
	<script src="http://code.jquery.com/jquery-1.10.2.min.js"></script>
	<script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
	<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.min.js"></script>
	{% block scripts %}{% endblock scripts %}
</head>
<body>
{% block navbartop %}
<nav class="navbar top" role="navigation">
	<div class="navbar-header"><button class="navbar-toggle" data-toggle="collapse" data-target="#navbar-top">
			<span class="sr-only"></span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
		</button><a href="" class="navbar-brand"></a></div>
	<div class="collapse navbar-collapse" id="navbar-top">
		<ul class="nav navbar-nav">
			<li><a href=""></a></li>
			<li><a href=""><i class="icon-home"></i> AlloFilm</a></li>
			<li><a href=""><i class="icon-bubble"></i> 0</a></li>
			<li><a href=""><i class="icon-plus"></i> Créer</a></li>
		</ul>
		<ul class="nav navbar-nav navbar-right">
			<li>
				<a href="">Salutations <span class="caret"></span></a>
				<ul class="dropdown-menu">
					<li><a href="">Mon profil</a></li>
					<li class="divider"></li>
					<li><a href="">Se déconnecter</a></li>
				</ul>
			</li>
		</ul>
	</div>
</nav>
{% endblock navbartop %}
<div class="clearfix"></div>
<div id="adminmenuback"></div>
<div id="adminmenuwrap">
{% block navbarleft %}
<ul class="nav nav-pills nav-stacked">
	<li><a href=""><i class="icon-dashboard"></i> Tableau de bord</a></li>
	<li><a href=""><i class="icon-pushpin"></i> Articles</a></li>
	<li><a href=""><i class="icon-image"></i> Médias</a></li>
	<li><a href=""><i class="icon-bubble"></i> Commentaires</a></li>
	<li><a href=""><i class="icon-hammer"></i> Apparence</a></li>
	<li><a href=""><i class="icon-settings"></i> Réglages</a></li>
</ul>	
{% endblock navbarleft %}
</div>
<div class="container-admin">
	<div class="col-lg-11 col-md-10 col-sm-10 col-xs-11">
		{% block body %}{% endblock body %}
	</div>
</div>
{% block footer %}
{% endblock footer %}
{% block javascripts %}
{% endblock javascripts %}	
</body>
</html>